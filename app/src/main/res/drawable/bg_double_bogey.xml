<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <stroke
                android:width="1dp"
                android:color="@android:color/holo_blue_light" />
            <solid android:color="@color/white"/>
        </shape>
    </item>

    <item
        android:top="2dp"
        android:right="2dp"
        android:left="2dp"
        android:bottom="2dp">
        <shape>
            <stroke
                android:width="1dp"
                android:color="@android:color/holo_blue_light" />
        </shape>
    </item>
</layer-list>
